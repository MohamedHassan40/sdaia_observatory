{
    "version": 2,
    "builds": [
      {
        "src": "frontend/next.config.mjs",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "https://your-api-url.com"
    },
    "rewrites": [
      {
        "source": "/dashboard",
        "destination": "/dashboard"
      }
    ]
  }
    